{
    "name": "gldrenthe89/nova-calculated-field",
    "description": "A server side calculated field for Laravel Nova",
    "keywords": [
        "laravel",
        "nova",
        "field",
        "calculated",
        "server-side"
    ],
    "license": "MIT",
    "repositories":[
        {
            "type": "path",
            "url": "../nova/nova-3.29.0"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "laravel/nova": "*",
        "laravel/framework": "^7.0|^8.0"
    },

    "autoload": {
        "psr-4": {
            "Gldrenthe89\\NovaCalculatedField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gldrenthe89\\NovaCalculatedField\\NovaCalculatedFieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
